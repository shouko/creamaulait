{
    "env": {
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "airbnb-base",
      "next/core-web-vitals"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2022
    },
    "rules": {
      "func-names": "off",
      "no-console": "off"
    }
}